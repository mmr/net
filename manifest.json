{
  "manifest_version": 2,
  "name": "NetU",
  "version": "1.0",
  "description": "Net Usage",
  "browser_action": {
    "default_icon": "res/imgs/icon.png",
    "default_popup": "src/popup.html"
  },
  "content_scripts": [{
    "js": [
      "src/popup.js"
    ],
    "matches": [
      "http://*/"
    ]
  }],
  "permissions": [
    "http://*/",
    "storage"
  ]
}
